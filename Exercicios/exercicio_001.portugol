programa
{

    funcao inicio()
    {
    real area,altura,base

    escreva ("Digite a altura em centimentros do retangulo: ")
    leia(altura)

    escreva ("Digite a base em centimentros do retangulo: ")
    leia(base)

    area = base*altura
    escreva("Este retangulo possui "+area+ " centimetros quadrados")
    }
}
